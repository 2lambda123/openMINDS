{
    "$id": "https://openminds.ebrains.eu/core/tissueSample?format=json-schema",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "_categories": [
        "fileOrigin"
    ],
    "_type": "https://openminds.ebrains.eu/core/TissueSample",
    "description": "Structured information on a tissue sample.",
    "properties": {
        "@id": {
            "description": "Metadata node identifier.",
            "type": "string"
        },
        "@type": {
            "const": "https://openminds.ebrains.eu/core/TissueSample",
            "type": "string"
        },
        "https://openminds.ebrains.eu/vocab/anatomicalLocation": {
            "_instruction": "Add all anatomical entities that describe the anatomical location of this tissue sample.",
            "items": {
                "else": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                },
                "if": {
                    "required": [
                        "@type"
                    ]
                },
                "then": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        },
                        "@type": {
                            "enum": [
                                "https://openminds.ebrains.eu/sands/ParcellationEntity",
                                "https://openminds.ebrains.eu/sands/ParcellationEntityVersion",
                                "https://openminds.ebrains.eu/sands/CustomAnatomicalEntity",
                                "https://openminds.ebrains.eu/controlledTerms/Organ",
                                "https://openminds.ebrains.eu/controlledTerms/OrganismSubstance",
                                "https://openminds.ebrains.eu/controlledTerms/UBERONParcellation",
                                "https://openminds.ebrains.eu/controlledTerms/SubcellularEntity",
                                "https://openminds.ebrains.eu/controlledTerms/CellType"
                            ],
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                }
            },
            "minItems": 1,
            "title": "anatomicalLocation",
            "type": "array",
            "uniqueItems": true
        },
        "https://openminds.ebrains.eu/vocab/biologicalSex": {
            "_instruction": "Add the biological sex of this specimen.",
            "description": "Differentiation of individuals of most species (animals and plants) based on the type of gametes they produce.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/controlledTerms/BiologicalSex"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "biologicalSex",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/internalIdentifier": {
            "_instruction": "Enter the identifier (or label) of this specimen that is used within the corresponding data files to identify this specimen.",
            "description": "Term or code that identifies someone or something within a particular product.",
            "title": "internalIdentifier",
            "type": "string"
        },
        "https://openminds.ebrains.eu/vocab/isPartOf": {
            "_instruction": "Add all tissue sample collections this tissue sample is part of.",
            "description": "Reference to the ensemble of multiple things or beings.",
            "items": {
                "else": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                },
                "if": {
                    "required": [
                        "@type"
                    ]
                },
                "then": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        },
                        "@type": {
                            "enum": [
                                "https://openminds.ebrains.eu/core/TissueSampleCollection"
                            ],
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                }
            },
            "minItems": 1,
            "title": "isPartOf",
            "type": "array",
            "uniqueItems": true
        },
        "https://openminds.ebrains.eu/vocab/laterality": {
            "_instruction": "Add one or both sides of the body, bilateral organ or bilateral organ part that this tissue sample originates from.",
            "description": "Differentiation between a pair of lateral homologous parts of the body.",
            "items": {
                "else": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                },
                "if": {
                    "required": [
                        "@type"
                    ]
                },
                "then": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        },
                        "@type": {
                            "enum": [
                                "https://openminds.ebrains.eu/controlledTerms/Laterality"
                            ],
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                }
            },
            "maxItems": 2,
            "minItems": 1,
            "title": "laterality",
            "type": "array",
            "uniqueItems": true
        },
        "https://openminds.ebrains.eu/vocab/lookupLabel": {
            "_instruction": "Enter a lookup label for this specimen that may help you to find this instance more easily.",
            "title": "lookupLabel",
            "type": "string"
        },
        "https://openminds.ebrains.eu/vocab/origin": {
            "_instruction": "Add the biogical origin of this tissue sample.",
            "description": "Source at which something begins or rises, or from which something derives.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/controlledTerms/CellType",
                            "https://openminds.ebrains.eu/controlledTerms/Organ",
                            "https://openminds.ebrains.eu/controlledTerms/OrganismSubstance"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "origin",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/species": {
            "_instruction": "Add the species or strain (a sub-type of a genetic variant of species) of this specimen.",
            "description": "Category of biological classification comprising related organisms or populations potentially capable of interbreeding, and being designated by a binomial that consists of the name of a genus followed by a Latin or latinized uncapitalized noun or adjective.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/controlledTerms/Species",
                            "https://openminds.ebrains.eu/core/Strain"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "species",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/studiedState": {
            "_instruction": "Add all states in which this tissue sample was studied.",
            "description": "Reference to a point in time at which something or someone was studied in a particular mode or condition.",
            "items": {
                "else": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                },
                "if": {
                    "required": [
                        "@type"
                    ]
                },
                "then": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        },
                        "@type": {
                            "enum": [
                                "https://openminds.ebrains.eu/core/TissueSampleState"
                            ],
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                }
            },
            "minItems": 1,
            "title": "studiedState",
            "type": "array",
            "uniqueItems": true
        },
        "https://openminds.ebrains.eu/vocab/type": {
            "_instruction": "Add the type of this tissue sample.",
            "description": "Distinct class to which a group of entities or concepts with similar characteristics or attributes belong to.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/controlledTerms/TissueSampleType"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "type",
            "type": "object"
        }
    },
    "required": [
        "https://openminds.ebrains.eu/vocab/species",
        "@id",
        "https://openminds.ebrains.eu/vocab/type",
        "https://openminds.ebrains.eu/vocab/studiedState",
        "@type",
        "https://openminds.ebrains.eu/vocab/origin"
    ],
    "type": "object"
}