{
    "$id": "https://openminds.ebrains.eu/computation/dataAnalysis?format=json-schema",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "_type": "https://openminds.ebrains.eu/computation/DataAnalysis",
    "properties": {
        "@id": {
            "description": "Metadata node identifier.",
            "type": "string"
        },
        "@type": {
            "const": "https://openminds.ebrains.eu/computation/DataAnalysis",
            "type": "string"
        },
        "https://openminds.ebrains.eu/vocab/environment": {
            "_instruction": "Add the computational environment in which this computation was executed.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/computation/Environment"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "environment",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/input": {
            "description": "Something or someone that is put into or participates in a process or machine.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/core/FileBundle",
                            "https://openminds.ebrains.eu/core/File",
                            "https://openminds.ebrains.eu/core/SoftwareVersion"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "input",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/launchConfiguration": {
            "_instruction": "Add the launch configuration of this computation, e.g. command-line arguments",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/computation/LaunchConfiguration"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "launchConfiguration",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/output": {
            "description": "Something or someone that comes out of, is delivered or produced by a process or machine.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/core/FileBundle",
                            "https://openminds.ebrains.eu/core/File"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "output",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/resourceUsage": {
            "anyOf": [
                {
                    "_type": "https://openminds.ebrains.eu/core/QuantitativeValue",
                    "description": "Structured information on a quanitative value.",
                    "properties": {
                        "uncertainty": {
                            "_instruction": "Enter the measurement uncertainty of this quantitative value.",
                            "description": "Quanitative value range defining the uncertainty of a measurement.",
                            "items": {
                                "type": "number"
                            },
                            "maxItems": 2,
                            "minItems": 2,
                            "title": "uncertainty",
                            "type": "array"
                        },
                        "unit": {
                            "_instruction": "Add the unit of measurement of this quantitative value.",
                            "_linkedTypes": [
                                "https://openminds.ebrains.eu/controlledTerms/UnitOfMeasurement"
                            ],
                            "description": "Determinate quantity adopted as a standard of measurement.",
                            "title": "unit"
                        },
                        "value": {
                            "_instruction": "Enter the measurement value of this quantitative value.",
                            "description": "Entry for a property.",
                            "title": "value",
                            "type": "number"
                        }
                    },
                    "required": [
                        "value"
                    ],
                    "title": "Quantitative value",
                    "type": "object"
                },
                {
                    "_type": "https://openminds.ebrains.eu/core/QuantitativeValueRange",
                    "properties": {
                        "maxValue": {
                            "_type": "https://openminds.ebrains.eu/core/QuantitativeValue",
                            "description": "Structured information on a quanitative value.",
                            "properties": {
                                "uncertainty": {
                                    "_instruction": "Enter the measurement uncertainty of this quantitative value.",
                                    "description": "Quanitative value range defining the uncertainty of a measurement.",
                                    "items": {
                                        "type": "number"
                                    },
                                    "maxItems": 2,
                                    "minItems": 2,
                                    "title": "uncertainty",
                                    "type": "array"
                                },
                                "unit": {
                                    "_instruction": "Add the unit of measurement of this quantitative value.",
                                    "_linkedTypes": [
                                        "https://openminds.ebrains.eu/controlledTerms/UnitOfMeasurement"
                                    ],
                                    "description": "Determinate quantity adopted as a standard of measurement.",
                                    "title": "unit"
                                },
                                "value": {
                                    "_instruction": "Enter the measurement value of this quantitative value.",
                                    "description": "Entry for a property.",
                                    "title": "value",
                                    "type": "number"
                                }
                            },
                            "required": [
                                "value"
                            ],
                            "title": "Quantitative value",
                            "type": "object"
                        },
                        "minValue": {
                            "_type": "https://openminds.ebrains.eu/core/QuantitativeValue",
                            "description": "Structured information on a quanitative value.",
                            "properties": {
                                "uncertainty": {
                                    "_instruction": "Enter the measurement uncertainty of this quantitative value.",
                                    "description": "Quanitative value range defining the uncertainty of a measurement.",
                                    "items": {
                                        "type": "number"
                                    },
                                    "maxItems": 2,
                                    "minItems": 2,
                                    "title": "uncertainty",
                                    "type": "array"
                                },
                                "unit": {
                                    "_instruction": "Add the unit of measurement of this quantitative value.",
                                    "_linkedTypes": [
                                        "https://openminds.ebrains.eu/controlledTerms/UnitOfMeasurement"
                                    ],
                                    "description": "Determinate quantity adopted as a standard of measurement.",
                                    "title": "unit"
                                },
                                "value": {
                                    "_instruction": "Enter the measurement value of this quantitative value.",
                                    "description": "Entry for a property.",
                                    "title": "value",
                                    "type": "number"
                                }
                            },
                            "required": [
                                "value"
                            ],
                            "title": "Quantitative value",
                            "type": "object"
                        }
                    },
                    "required": [
                        "maxValue",
                        "minValue"
                    ],
                    "title": "Quantitative value range",
                    "type": "object"
                }
            ]
        },
        "https://openminds.ebrains.eu/vocab/startedBy": {
            "_instruction": "Add the person or software agent who launched this computation.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/core/Person",
                            "https://openminds.ebrains.eu/computation/SoftwareAgent"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "startedBy",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/status": {
            "_instruction": "Enter the current status of this computation.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/controlledTerms/ActionStatusType"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "status",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/tags": {
            "_instruction": "Enter custom tags for this computation.",
            "items": {
                "type": "string"
            },
            "minItems": 1,
            "title": "tags",
            "type": "array",
            "uniqueItems": true
        },
        "https://openminds.ebrains.eu/vocab/wasInformedBy": {
            "_instruction": "Add any other computation that sent data to this one during runtime.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/computation/Computation"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "wasInformedBy",
            "type": "object"
        }
    },
    "required": [
        "output",
        "@id",
        "@type",
        "launchConfiguration",
        "startedAtTime",
        "environment",
        "input"
    ],
    "title": "Data analysis",
    "type": "object"
}