{
    "$id": "https://openminds.ebrains.eu/sands/anatomicalEntityRelation?format=json-schema",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "_deprecated": true,
    "_type": "https://openminds.ebrains.eu/sands/AnatomicalEntityRelation",
    "description": "Structured information on the relation between one anatomical entity and another.",
    "properties": {
        "@id": {
            "description": "Metadata node identifier.",
            "type": "string"
        },
        "@type": {
            "const": "https://openminds.ebrains.eu/sands/AnatomicalEntityRelation",
            "type": "string"
        },
        "https://openminds.ebrains.eu/vocab/criteria": {
            "_instruction": "Add the protocol execution defining the criteria that were applied to determine this relation.",
            "description": "Aspects or standards on which a judgement or decision is based.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/core/ProtocolExecution"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "criteria",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/criteriaQualityType": {
            "_instruction": "Add the quality type of the stated criteria used to determine this relation.",
            "description": "Distinct class that defines how the judgement or decision was made for a particular criteria.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/controlledTerms/CriteriaQualityType"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "criteriaQualityType",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/inRelationTo": {
            "_instruction": "Add the anatomical entity to which the relation is described.",
            "description": "Reference to a related element.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/sands/AnatomicalEntity"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "inRelationTo",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/qualitativeOverlap": {
            "_instruction": "Add the qualitative overlap that best describes the relation between the two anatomical entities.",
            "description": "Semantic characterization of how much two things occupy the same space.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/controlledTerms/QualitativeOverlap"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "qualitativeOverlap",
            "type": "object"
        },
        "https://openminds.ebrains.eu/vocab/quantitativeOverlap": {
            "anyOf": [
                {
                    "_type": "https://openminds.ebrains.eu/core/QuantitativeValue",
                    "description": "Structured information on a quantitative value.",
                    "properties": {
                        "uncertainty": {
                            "_instruction": "Enter the measurement uncertainty of this quantitative value.",
                            "description": "Quantitative value range defining the uncertainty of a measurement.",
                            "items": {
                                "type": "number"
                            },
                            "maxItems": 2,
                            "minItems": 2,
                            "title": "uncertainty",
                            "type": "array"
                        },
                        "unit": {
                            "_instruction": "Add the unit of measurement of this quantitative value.",
                            "_linkedTypes": [
                                "https://openminds.ebrains.eu/controlledTerms/UnitOfMeasurement"
                            ],
                            "description": "Determinate quantity adopted as a standard of measurement.",
                            "title": "unit"
                        },
                        "value": {
                            "_instruction": "Enter the measurement value of this quantitative value.",
                            "description": "Entry for a property.",
                            "title": "value",
                            "type": "number"
                        }
                    },
                    "required": [
                        "value"
                    ],
                    "type": "object"
                },
                {
                    "_type": "https://openminds.ebrains.eu/core/QuantitativeValueRange",
                    "description": "A representation of a range of quantitative values.",
                    "properties": {
                        "maxValue": {
                            "_type": "https://openminds.ebrains.eu/core/QuantitativeValue",
                            "description": "Structured information on a quantitative value.",
                            "properties": {
                                "uncertainty": {
                                    "_instruction": "Enter the measurement uncertainty of this quantitative value.",
                                    "description": "Quantitative value range defining the uncertainty of a measurement.",
                                    "items": {
                                        "type": "number"
                                    },
                                    "maxItems": 2,
                                    "minItems": 2,
                                    "title": "uncertainty",
                                    "type": "array"
                                },
                                "unit": {
                                    "_instruction": "Add the unit of measurement of this quantitative value.",
                                    "_linkedTypes": [
                                        "https://openminds.ebrains.eu/controlledTerms/UnitOfMeasurement"
                                    ],
                                    "description": "Determinate quantity adopted as a standard of measurement.",
                                    "title": "unit"
                                },
                                "value": {
                                    "_instruction": "Enter the measurement value of this quantitative value.",
                                    "description": "Entry for a property.",
                                    "title": "value",
                                    "type": "number"
                                }
                            },
                            "required": [
                                "value"
                            ],
                            "type": "object"
                        },
                        "minValue": {
                            "_type": "https://openminds.ebrains.eu/core/QuantitativeValue",
                            "description": "Structured information on a quantitative value.",
                            "properties": {
                                "uncertainty": {
                                    "_instruction": "Enter the measurement uncertainty of this quantitative value.",
                                    "description": "Quantitative value range defining the uncertainty of a measurement.",
                                    "items": {
                                        "type": "number"
                                    },
                                    "maxItems": 2,
                                    "minItems": 2,
                                    "title": "uncertainty",
                                    "type": "array"
                                },
                                "unit": {
                                    "_instruction": "Add the unit of measurement of this quantitative value.",
                                    "_linkedTypes": [
                                        "https://openminds.ebrains.eu/controlledTerms/UnitOfMeasurement"
                                    ],
                                    "description": "Determinate quantity adopted as a standard of measurement.",
                                    "title": "unit"
                                },
                                "value": {
                                    "_instruction": "Enter the measurement value of this quantitative value.",
                                    "description": "Entry for a property.",
                                    "title": "value",
                                    "type": "number"
                                }
                            },
                            "required": [
                                "value"
                            ],
                            "type": "object"
                        }
                    },
                    "required": [
                        "maxValue",
                        "minValue"
                    ],
                    "type": "object"
                }
            ]
        }
    },
    "required": [
        "@type",
        "@id",
        "https://openminds.ebrains.eu/vocab/criteriaQualityType",
        "https://openminds.ebrains.eu/vocab/inRelationTo",
        "https://openminds.ebrains.eu/vocab/qualitativeOverlap"
    ],
    "type": "object"
}