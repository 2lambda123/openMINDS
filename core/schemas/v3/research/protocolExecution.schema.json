{
    "$id": "https://openminds.ebrains.eu/core/protocolExecution?format=json-schema",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "_type": "https://openminds.ebrains.eu/core/ProtocolExecution",
    "description": "Structured information on a protocol execution.",
    "properties": {
        "@id": {
            "description": "Metadata node identifier.",
            "type": "string"
        },
        "@type": {
            "const": "https://openminds.ebrains.eu/core/ProtocolExecution",
            "type": "string"
        },
        "description": {
            "_instruction": "Enter a description of this protocol execution.",
            "title": "Description",
            "type": "string"
        },
        "input": {
            "_instruction": "Add all inputs (subject state, tissue sample state, file instance or file bundle) used by this protocol execution.",
            "items": {
                "else": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                },
                "if": {
                    "required": [
                        "@type"
                    ]
                },
                "then": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        },
                        "@type": {
                            "enum": [
                                "https://openminds.ebrains.eu/core/FileBundle",
                                "https://openminds.ebrains.eu/core/FileInstance",
                                "https://openminds.ebrains.eu/core/SubjectGroupState",
                                "https://openminds.ebrains.eu/core/SubjectState",
                                "https://openminds.ebrains.eu/core/TissueSampleCollectionState",
                                "https://openminds.ebrains.eu/core/TissueSampleState"
                            ],
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                }
            },
            "minItems": 1,
            "title": "Input",
            "type": "array",
            "uniqueItems": true
        },
        "output": {
            "_instruction": "Add all outputs (subject state, tissue sample state, file instance or file bundle) generated by this protocol execution.",
            "items": {
                "else": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                },
                "if": {
                    "required": [
                        "@type"
                    ]
                },
                "then": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        },
                        "@type": {
                            "enum": [
                                "https://openminds.ebrains.eu/core/FileBundle",
                                "https://openminds.ebrains.eu/core/FileInstance",
                                "https://openminds.ebrains.eu/core/SubjectGroupState",
                                "https://openminds.ebrains.eu/core/SubjectState",
                                "https://openminds.ebrains.eu/core/TissueSampleCollectionState",
                                "https://openminds.ebrains.eu/core/TissueSampleState"
                            ],
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                }
            },
            "minItems": 1,
            "title": "Output",
            "type": "array",
            "uniqueItems": true
        },
        "parameterSetting": {
            "_instruction": "Add all important parameter settings defining this protocol execution.",
            "items": {
                "else": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                },
                "if": {
                    "required": [
                        "@type"
                    ]
                },
                "then": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        },
                        "@type": {
                            "enum": [
                                "https://openminds.ebrains.eu/core/ParameterSetting"
                            ],
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                }
            },
            "minItems": 1,
            "title": "Parameter setting",
            "type": "array",
            "uniqueItems": true
        },
        "preparationType": {
            "_instruction": "Add the initial preparation type for this protocol execution.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/controlledTerms/PreparationType"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "Preparation type",
            "type": "object"
        },
        "protocol": {
            "_instruction": "Add the protocol of this protocol execution.",
            "else": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "if": {
                "required": [
                    "@type"
                ]
            },
            "then": {
                "properties": {
                    "@id": {
                        "format": "iri",
                        "type": "string"
                    },
                    "@type": {
                        "enum": [
                            "https://openminds.ebrains.eu/core/Protocol"
                        ],
                        "format": "iri",
                        "type": "string"
                    }
                },
                "required": [
                    "@id"
                ]
            },
            "title": "Protocol",
            "type": "object"
        },
        "semanticallyAnchoredTo": {
            "_instruction": "Add all anatomical entities to which the outputs of this protocol execution can be semantically anchored to.",
            "items": {
                "else": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                },
                "if": {
                    "required": [
                        "@type"
                    ]
                },
                "then": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        },
                        "@type": {
                            "enum": [
                                "https://openminds.ebrains.eu/sands/AnatomicalEntity"
                            ],
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                }
            },
            "minItems": 1,
            "title": "Semantically anchored to",
            "type": "array",
            "uniqueItems": true
        },
        "studyTarget": {
            "_instruction": "Add all study targets of this model version.",
            "items": {
                "else": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                },
                "if": {
                    "required": [
                        "@type"
                    ]
                },
                "then": {
                    "properties": {
                        "@id": {
                            "format": "iri",
                            "type": "string"
                        },
                        "@type": {
                            "enum": [
                                "https://openminds.ebrains.eu/controlledTerms/Genotype",
                                "https://openminds.ebrains.eu/controlledTerms/Strain",
                                "https://openminds.ebrains.eu/controlledTerms/Disease",
                                "https://openminds.ebrains.eu/controlledTerms/DiseaseModel",
                                "https://openminds.ebrains.eu/controlledTerms/Phenotype",
                                "https://openminds.ebrains.eu/controlledTerms/Species",
                                "https://openminds.ebrains.eu/controlledTerms/Organ",
                                "https://openminds.ebrains.eu/controlledTerms/TermSuggestion",
                                "https://openminds.ebrains.eu/controlledTerms/CellType",
                                "https://openminds.ebrains.eu/controlledTerms/BiologicalSex"
                            ],
                            "format": "iri",
                            "type": "string"
                        }
                    },
                    "required": [
                        "@id"
                    ]
                }
            },
            "minItems": 1,
            "title": "Study target",
            "type": "array",
            "uniqueItems": true
        }
    },
    "required": [
        "output",
        "@type",
        "protocol",
        "@id",
        "input"
    ],
    "title": "Protocol execution",
    "type": "object"
}